         <font>
            <Font name="DejaVu Sans Bold" size="18.0" />
         </font>
      </Button>
      <Label alignment="CENTER" focusTraversable="false" layoutX="150.0" layoutY="100.0" prefHeight="100.0" prefWidth="500.0" text="Attention, si vous supprimez votre compte, toutes vos données seront effacées !" textAlignment="CENTER" textFill="#b20000" wrapText="true" AnchorPane.topAnchor="100.0">
